<Configuration status="WARN">
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="%d [%t] %-5level %logger{36} - %msg%n%throwable"/>
    </Console>
</Appenders>
<Loggers>
    <Root level="trace">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
        </Configuration>